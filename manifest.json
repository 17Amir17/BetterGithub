{
  "name": "Better Github",
  "description": "A better version of github",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs"],
  "host_permissions": ["*://*.github.com/*"],
  "background": {
    "service_worker": "background.js",
    "presistant": false
  },
  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "js": ["main.js"]
    }
  ]
}
